{
	"name": "giselle-project",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"build": "turbo build",
		"build-sdk": "turbo build --filter '@giselles-ai/*'",
		"build-data-type": "turbo build --filter '@giselles-ai/protocol'",
		"check-types": "turbo check-types",
		"format": "biome check --write .",
		"clean": "turbo clean",
		"dev:studio.giselles.ai": "NODE_NO_WARNINGS=1 turbo dev --filter studio.giselles.ai",
		"changeset": "changeset",
		"version": "changeset version",
		"test": "turbo test",
		"tidy": "knip --no-config-hints",
		"strip-workspace": "pnpm -F strip-workspace strip-workspace",
		"prevd": "pnpm i && pnpm build-sdk",
		"vd": "vercel dev",
		"report:colors": "node scripts/report-colors.mjs",
		"report:colors:out": "node scripts/report-colors.mjs --out .reports/report-colors.json",
		"guard:colors": "node scripts/guard-colors.mjs",
		"lint:colors:code": "node scripts/lint-colors-code.mjs",
		"metrics:count": "node scripts/metrics-count.mjs",
		"codemod:text-black-600-20": "node scripts/codemods/replace-text-black-600-20.mjs",
		"codemod:text-black-600-20:apply": "node scripts/codemods/replace-text-black-600-20.mjs --apply",
		"codemod:text-white-900": "node scripts/codemods/replace-text-white-900-to-inverse.mjs",
		"codemod:text-white-900:apply": "node scripts/codemods/replace-text-white-900-to-inverse.mjs --apply",
		"codemod:safe-pass-1": "node scripts/codemods/safe-pass-1.mjs",
		"codemod:safe-pass-1:apply": "node scripts/codemods/safe-pass-1.mjs --apply",
		"report:compat-tokens": "node scripts/compat-tokens-inventory.mjs"
	},
	"devDependencies": {
		"@biomejs/biome": "catalog:",
		"@changesets/cli": "^2.28.1",
		"@types/node": "catalog:",
		"knip": "5.67.1",
		"turbo": "2.7.3",
		"typescript": "catalog:"
	},
	"packageManager": "pnpm@10.16.0",
	"engines": {
		"node": ">=24"
	},
	"pnpm": {
		"overrides": {
			"@electric-sql/client": "1.0.10",
			"cookie": "1.0.2",
			"@react-email/preview-server>next": "16.1.5",
			"vite": "7.1.3",
			"jws": "4.0.1",
			"qs": "6.14.1",
			"glob@>=11.0.0 <11.1.0": "11.1.0",
			"fast-xml-parser": "5.3.4",
			"@isaacs/brace-expansion": "5.0.1"
		}
	}
}
